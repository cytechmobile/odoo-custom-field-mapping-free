<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_interested_in_form" model="ir.ui.view">
        <field name="name">interested.in.form</field>
        <field name="model">interested.in</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_interested_in_tree" model="ir.ui.view">
        <field name="name">interested.in.tree</field>
        <field name="model">interested.in</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_interested_in" model="ir.actions.act_window">
        <field name="name">Interested In</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">interested.in</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>